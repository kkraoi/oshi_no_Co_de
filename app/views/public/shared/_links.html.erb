<%- if controller_name != 'sessions' %>
  <p>
    ログインは
    <%= link_to "こちら", new_session_path(resource_name), class: "u-link" %>
  </p>
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <p>
    サインアップは
    <%= link_to "こちら", new_registration_path(resource_name), class: "u-link" %><br />
  </p>
<% end %>

